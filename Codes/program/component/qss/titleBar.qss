TitleBar {
  background-color: #333;
}

TitleBar QLabel {
  color: #ffffff;
  font-size: 16px;
  text-align: center;
}

TitleBar QPushButton {
  color: #ffffff;
  margin-right: 3px;
  margin-top: 5px;
  max-width: 25px;
  height: 25px;
  float: right;
}

TitleBar QPushButton#minimizeButton {
  image: url(path/to/minimize_icon.png);
}

TitleBar QPushButton#maximizeButton {
  image: url(path/to/maximize_icon.png);
}

TitleBar QPushButton#closeButton {
  image: url(path/to/close_icon.png);
}
